{
  "seed": 5,
  "s3": {
    "p_bucket": "internal",
    "prefix": "warning"
  },
  "cpu": true,
  "project_key_name": "HydrographyProject",
  "testing": true,
  "scheduler_events": {
    "name": "HydrographyWarningDiscrete",
    "schedule_expression": "cron(45 09,21 * * ? *)",
